{
  "name": "Rocket - Article Saver",
  "description": "Easily save articles to Rocket app.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
      "16": "rocket.png",
      "32": "rocket.png",
      "48": "rocket.png",
      "128": "rocket.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "browser_action": {
      "default_icon": "rocket.png"
  },
  "permissions": [
      "tabs", 
      "http://*/*", 
      "https://*/*"
  ]
}